{
    "name": "hybrid-genesis",
    "displayName": "Hybrid GENESIS",
    "description": "AI-Driven Spatial Architecture Planner (Layer 0)",
    "version": "1.0.0",
    "publisher": "baroni",
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "hybrid-genesis.showGraph",
                "title": "Hybrid GENESIS: Show 3D Architectural Graph"
            }
        ]
